{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.js)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.png|.*\\.jpg|.*\\.gif|.*\\.ico)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/$1"
    },
    {
      "src": "/((?!api|static|.*\\..*|_next).*)",
      "dest": "/index.html"
    }
  ]
}